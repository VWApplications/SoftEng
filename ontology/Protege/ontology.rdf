    <!-- http://purl.org/dc/terms/provenance -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/provenance">
        <rdfs:comment xml:lang="pt">Uma declaração de quaisquer alterações na propriedade e custódia do recurso desde a sua criação que sejam significativas por sua autenticidade, integridade e interpretação.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
        <rdfs:label xml:lang="pt">Versionamento</rdfs:label>
    </owl:AnnotationProperty>
    


